{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@2.0.2",
            "config": {
                "php": "8.2",
                "phpIni": "api/php.ini"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://nama-proyek-kamu.vercel.app",

        "DB_CONNECTION": "mysql",
        "DB_HOST": "nama_host_database_kamu",
        "DB_PORT": "3306",
        "DB_DATABASE": "nama_database",
        "DB_USERNAME": "username_db",
        "DB_PASSWORD": "password_db",

        "APP_KEY": "base64:kunci_app_key_kamu_di_sini",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}